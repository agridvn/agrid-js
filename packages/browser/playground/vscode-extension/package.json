{
    "name": "agrid-vscode-playground",
    "displayName": "Agrid VS Code Playground",
    "description": "A playground for testing Agrid integration in a VS Code extension.",
    "version": "0.0.1",
    "publisher": "agrid",
    "engines": {
        "vscode": "^1.74.0"
    },
    "activationEvents": [
        "onCommand:agrid.start"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "agrid.start",
                "title": "Start Agrid Playground",
                "category": "Agrid"
            }
        ]
    },
    "devDependencies": {
        "vscode": "*"
    },
    "dependencies": {
        "dotenv": "^17.0.0"
    }
}
