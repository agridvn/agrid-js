{
    "name": "@agrid/react",
    "version": "1.4.0",
    "description": "Provides components and hooks for React integrations of Agrid.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/agrid/agrid-js.git",
        "directory": "packages/react"
    },
    "author": {
        "name": "Agrid",
        "email": "hey@agrid.com",
        "url": "https://agrid.com"
    },
    "license": "MIT",
    "homepage": "https://agrid.com/docs/libraries/react",
    "keywords": [
        "agrid",
        "react",
        "analytics",
        "feature-flags",
        "hooks"
    ],
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "pnpm@10.12.4",
    "scripts": {
        "clean": "rimraf dist",
        "build": "rollup -c",
        "dev": "rollup -cw",
        "test:unit": "jest",
        "lint": "eslint src",
        "test:debug": "jest --runInBand",
        "prepublishOnly": "pnpm lint && pnpm test:unit && pnpm build",
        "package": "pnpm pack --out $PACKAGE_DEST/%s.tgz"
    },
    "main": "dist/umd/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "peerDependencies": {
        "@types/react": ">=16.8.0",
        "agrid-js": ">=1.257.2",
        "react": ">=16.8.0"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    },
    "devDependencies": {
        "@babel/preset-react": "^7.18.6",
        "@agrid-tooling/rollup-utils": "workspace:*",
        "@rollup/plugin-inject": "^4.0.2",
        "@rollup/plugin-replace": "^2.3.4",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^11.2.2",
        "@testing-library/react-hooks": "^8.0.1",
        "@types/react": "^17.0.0",
        "cross-env": "^7.0.3",
        "jest": "catalog:",
        "jest-environment-jsdom": "catalog:",
        "agrid-js": "workspace:*",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-test-renderer": "^17.0.1",
        "rollup": "^2.35.1",
        "rollup-plugin-copy": "^3.5.0",
        "tslib": "catalog:",
        "typescript": "catalog:"
    }
}
