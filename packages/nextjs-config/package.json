{
  "name": "@agrid/nextjs-config",
  "version": "1.4.3",
  "description": "NextJS configuration helper for Agrid - Product analytics and feature flags",
  "repository": {
    "type": "git",
    "url": "https://github.com/agridvn/agrid-js.git",
    "directory": "packages/nextjs-config"
  },
  "author": {
    "name": "Agrid",
    "email": "info@agrid.vn",
    "url": "https://agrid.com"
  },
  "homepage": "https://agrid.com/docs/libraries/nextjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "pnpm lint && pnpm build",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "rslib build",
    "test:unit": "jest --passWithNoTests",
    "dev": "rslib build --watch",
    "package": "pnpm pack --out $PACKAGE_DEST/%s.tgz"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "engines": {
    "node": ">=20"
  },
  "license": "MIT",
  "dependencies": {
    "@posthog/cli": "catalog:",
    "semver": "^7.7.2",
    "@agrid/core": "workspace:*"
  },
  "keywords": [
    "agrid",
    "nextjs",
    "analytics",
    "feature-flags"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@agrid-tooling/rollup-utils": "workspace:*",
    "@agrid-tooling/tsconfig-base": "workspace:*",
    "@rslib/core": "catalog:",
    "@types/node": "^22.15.23",
    "@types/semver": "^7.7.0",
    "jest": "catalog:",
    "next": "^15.4.1",
    "ts-jest": "catalog:"
  },
  "peerDependencies": {
    "next": ">12.1.0"
  },
  "files": [
    "dist/"
  ]
}
