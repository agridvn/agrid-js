{
  "name": "@agrid/nuxt",
  "version": "1.1.7",
  "description": "Nuxt module for Agrid - Product analytics and feature flags",
  "author": {
    "name": "Agrid",
    "email": "info@agrid.vn",
    "url": "https://agrid.vn"
  },
  "homepage": "https://github.com/agridvn/agrid-js#readme",
  "keywords": [
    "agrid",
    "nuxt",
    "analytics",
    "feature-flags"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agridvn/agrid-js.git",
    "directory": "packages/nuxt"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxt prepare || true",
    "build": "nuxt-module-build build",
    "package": "pnpm pack --out $PACKAGE_DEST/%s.tgz",
    "lint": "eslint --no-ignore \"src/**/*.{ts,js,mjs}\""
  },
  "dependencies": {
    "@nuxt/kit": ">=3.7.0",
    "agrid-node": "workspace:*",
    "@agrid/core": "workspace:*",
    "agrid-js": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "8.57.0",
    "@types/node": "^20.0.0",
    "nuxt": "^4.1.2",
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.2",
    "eslint": "8.57.0"
  }
}
