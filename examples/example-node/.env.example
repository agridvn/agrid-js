# Agrid API Configuration
# Copy this file to .env and update with your actual values

# Your project API key (found on the /setup page in Agrid)
AGRID_PROJECT_API_KEY=phc_your_project_api_key_here

# Your personal API key (for local evaluation and other advanced features)
AGRID_PERSONAL_API_KEY=phx_your_personal_api_key_here

# Agrid host URL
AGRID_HOST=https://app.agrid.vn

# Test flag key for diagnostics (optional - any key works, even non-existent ones)
AGRID_TEST_FLAG_KEY=beta-feature
